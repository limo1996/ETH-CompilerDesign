/* Test all binary expressions */
class Main {
    void main() {
        int i0,  i1,  i2;

        // +
        i0 = +2 + +3;
        i1 = 0 - +i1;
        i2 = +i0 * +i1;
        i1 = +i0 * 1;
        i0 = +0 + +(+i0);
        i1 = +(+(i0));

        write(i0); writeln();
        write(i1); writeln();
        write(i2); writeln();

        // -
        i0 = -6 - 1;
        i2 = i0 * -i1;
        i2 = -(-(1));

        write(i0); writeln();
        write(i1); writeln();
        write(i2); writeln();

        i0 = -(+3);
        i1 = +(-(+(-(-4))));
        i0 = -i1 -(-(+(i1)));

        write(i0); writeln();
        write(i1); writeln();
        write(i2); writeln();
    }
}
